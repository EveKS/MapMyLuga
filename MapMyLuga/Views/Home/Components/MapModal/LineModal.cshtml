                    <div class="input-group w100pr">
                        <span class="input-group-addon w100 map-modal-group-name">Группа:</span>
                        <select class="form-control map-modal-group-input" id="MarkerGroupIdLine" name="MarkerGroupId" autocomplete="off">
                            @foreach (MarkerGroup item in ViewBag.MarkerGroup)
                            {
                                <option value="@item.MarkerGroupId">@item.GroupName</option>
                            }
                        </select>
                    </div>
                    <div class="input-group w100pr">
                        <input asp-for="MarkerTitle" type="text" class="form-control map-modal-title" id="MarkerTitleLine" autocomplete="off" placeholder="Заголовок" />
                        <span asp-validation-for="MarkerTitle"></span>
                    </div>
                    <div class="input-group w100pr">
                        <textarea asp-for="MarkerDescription" rows="6" class="form-control map-modal-description" id="MarkerDescriptionLine" autocomplete="off" placeholder="Описание"></textarea>
                        <span asp-validation-for="MarkerDescription"></span>
                    </div>
                    <span class="help-block">
                        Выберите одно или несколько изображений
                    </span>
