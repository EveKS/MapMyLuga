    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header map-modal-head">
                <button type="button" class="close map-modal-close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title text-center">Информация об отметке</h4>
            </div>
            <form id="markerMessageFormLine" name="markerMessageForm" method="post" asp-action="Post" asp-controller="Home" enctype="multipart/form-data">
