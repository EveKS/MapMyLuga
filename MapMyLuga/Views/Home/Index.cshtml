@{
    ViewData["Title"] = "Home Page";
}

<vc:map-menu></vc:map-menu>
<vc:map-view></vc:map-view>
<vc:add-markers></vc:add-markers>

<section style="display: none;">
    <div class="container">
        <div class="row">
            <div class="col-md-12 map_message">
                <p id="message">
                </p>
            </div>
        </div>
    </div>
</section>

@section Scripts{
    <environment include="Development">
        <script src="~/js/googleMap.es5.js" asp-append-version="true"></script>
        <script src="~/js/AddLine.es5.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/map.min.js" asp-append-version="true"></script>
    </environment>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyArhXIMVjmk7mp9wmVNYsUuTdCcWn7bycE&libraries=places&callback=initMap" async defer asp-append-version="true"></script>
}
